{
  "manifest_version": 2,
  "version": "1.0",
  "name": "Colors",
  "description": "A color picker utitlity for 100r themes",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "logo192.png",
    "default_popup": "index.html"
  },
  "permissions": ["<all_urls>", "tabs", "storage"],
  "icons": {
    "192": "logo192.png"
  }
}
